{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Start Jeopardy Dev Server",
        "type": "shell",
        "command": "npm run dev",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "isBackground": true,
        "problemMatcher": []
      },
      {
        "label": "Start Proxy",
        "type": "shell",
        "command": "node proxy.js",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "isBackground": true,
        "problemMatcher": []
      },
      {
        "label": "Start All",
        "dependsOn": [
          "Start Jeopardy Dev Server",
          "Start Proxy",
          "Start Browser Tools Server"
        ],
        "dependsOrder": "parallel"
      },
      {
        "label": "Start Browser Tools Server",
        "type": "shell",
        "command": "npx @agentdeskai/browser-tools-server@1.2.1",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "isBackground": true,
        "problemMatcher": []
      },
      {
        "label": "Install React Type Definitions",
        "type": "shell",
        "command": "npm install --save-dev @types/react @types/react-dom",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "problemMatcher": []
      }
    ]
  }