{
  "name": "jeopardy-lang",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "proxy": "node proxy.js",
    "browser-tools-server": "npx @agentdeskai/browser-tools-server@1.2.1",
    "start": "npm-run-all --parallel dev proxy browser-tools-server",
    "start:local": "npm-run-all --parallel dev proxy",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:chrome:headed": "playwright test --project=chromium --headed",
    "test:e2e:chrome:debug": "playwright test --project=chromium --debug",
    "test:refactored": "playwright test --config=playwright.config.refactored.ts",
    "test:refactored:ui": "playwright test --config=playwright.config.refactored.ts --ui",
    "test:refactored:headed": "playwright test --config=playwright.config.refactored.ts --headed",
    "test:refactored:debug": "playwright test --config=playwright.config.refactored.ts --debug",
    "test:smoke": "playwright test --config=playwright.config.refactored.ts --grep smoke",
    "test:renderers": "playwright test --config=playwright.config.refactored.ts renderer-tests.spec.ts",
    "test:homepage": "playwright test --config=playwright.config.refactored.ts refactored-homepage.spec.ts",
    "test:gameflow": "playwright test --config=playwright.config.refactored.ts refactored-game-flow.spec.ts",
    "test:report": "playwright show-report",
    "test:install": "playwright install"
  },
  "dependencies": {
    "@react-three/drei": "^9.102.6",
    "@react-three/fiber": "^8.15.19",
    "express": "^5.1.0",
    "framer-motion": "^11.0.0",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.162.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.178.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.0.0",
    "vite": "^7.0.4"
  }
}
